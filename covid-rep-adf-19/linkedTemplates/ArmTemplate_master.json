{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-rep-adf-19"
		},
		"ls_ablob_covidrepsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidrepsa'"
		},
		"ls_adls_covidrepdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidrepdl'"
		},
		"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/hbuddana/Azure_Data_Factory_COVID-19_Reporting/main/Datasets%20and%20Scripts/ecdc_data/hospital_admissions.csv"
		},
		"ls_adls_covidrepdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidrepadls19.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-rep-adf-19_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidrepsa_connectionString": {
						"value": "[parameters('ls_ablob_covidrepsa_connectionString')]"
					},
					"ls_adls_covidrepdl_accountKey": {
						"value": "[parameters('ls_adls_covidrepdl_accountKey')]"
					},
					"Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('Is_http_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"ls_adls_covidrepdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidrepdl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}